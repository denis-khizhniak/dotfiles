pcm.!default {
  type plug
  slave.pcm "dmixer"
}

pcm.hdmi {
  type hw
  card HDMI
  device 3
}

pcm.dmixer  {
  type dmix
  ipc_key 1024
  slave {
    pcm "hdmi"
    period_time 0
    period_size 1024
    buffer_size 4096
    rate 44100
  }
  bindings {
    0 3 
  }
}

ctl.dmixer {
  type hw
  card HDMI
  device 3
}
